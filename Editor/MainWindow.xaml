        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button" Content="Nahrát Obrázek" HorizontalAlignment="Left" Height="25" Margin="30,25,0,0" VerticalAlignment="Top" Width="100" Click="button_Click"/>
