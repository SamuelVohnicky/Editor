        <Button x:Name="button_Copy" Content="Modra" HorizontalAlignment="Left" Height="25" Margin="30,97,0,0" VerticalAlignment="Top" Width="100" Click="button_Copy_Click"/>
        <Button x:Name="button_Copy1" Content="Placeholder" HorizontalAlignment="Left" Height="25" Margin="30,127,0,0" VerticalAlignment="Top" Width="100" Click="button_Copy1_Click"/>
        <Button x:Name="button_Copy2" Content="Placeholder" HorizontalAlignment="Left" Height="25" Margin="30,157,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="button_Copy3" Content="Placeholder" HorizontalAlignment="Left" Height="25" Margin="30,187,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="button_Copy4" Content="Placeholder" HorizontalAlignment="Left" Height="25" Margin="30,217,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Window>
